{
    "env": {
        "localPaths": [
            "${workspaceFolder}/chat",
            "${workspaceFolder}/common",
            "${workspaceFolder}/contacts",
            "${workspaceFolder}/interface",
            "${workspaceFolder}/textbox"
        ],
        "protobufPaths": [
            "${workspaceFolder}/build"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${localPaths}",
                "${protobufPaths}"
            ],
            "compilerPath": "/usr/bin/c++",
            "intelliSenseMode": "linux-gcc-x86",
            "cppStandard": "c++20",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}